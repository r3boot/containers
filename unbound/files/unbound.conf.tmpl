  # rkt zone
  local-zone: "rkt." static
  local-data: "rkt. 10800 IN NS unbound.rkt."
  local-data: "rkt. 10800 IN SOA rkt. dnsmaster.rkt. 1 3600 1200 604800 10800"
  local-data: "rkt. 10800 IN A 10.0.4.1"
  local-data: "localhost.rkt. 10800 IN A 127.0.0.1"
  local-data: "localhost.rkt. 10800 IN AAAA ::1"
  local-data: "alita.rkt. 10800 IN A 10.0.4.1"
  local-data: "registrator.rkt. 10800 IN A 10.0.4.251"
  local-data: "consul.rkt. 10800 IN A 10.0.4.252"
  local-data: "etcd.rkt. 10800 IN A 10.0.4.253"
  local-data: "unbound.rkt. 10800 IN A 10.0.4.254"

  # 4.0.10.in-addr.arpa zone
